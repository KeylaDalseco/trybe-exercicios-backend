  "bugs": {
    "url": "https://github.com/betrybe/proposal-trybecar/issues"
  },
  "homepage": "https://github.com/betrybe/proposal-trybecar#readme",
  "devDependencies": {
    "eslint-config-trybe-backend": "2.1.0",
    "chai": "4.3.4",
    "chai-http": "4.3.0",
    "mocha": "10.2.0",
    "nodemon": "^2.0.22",
    "nyc": "15.1.0",
    "sinon": "11.1.1",
    "sinon-chai": "3.7.0"
  },
  "dependencies": {
    "camelize": "1.0.1",
    "express": "4.18.2",
    "joi": "17.6.0",
