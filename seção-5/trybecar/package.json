    "test:coverage": "nyc --all --include src/models --include src/services --include src/controllers mocha tests/unit/**/*.js --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betrybe/proposal-trybecar.git"
  },
